{
  "manifest_version": 3,
  "name": "인프런 커뮤니티 답변 정렬기",
  "version": "1.0.0",
  "icons": {
    "16": "images/icons/inflearn_logo.png",
    "48": "images/icons/inflearn_logo.png",
    "128": "images/icons/inflearn_logo.png"
  },
  "content_scripts": [
    {
      "matches": ["https://www.inflearn.com/*"],
      "js": ["content.js"]
    }
  ],
  "permissions": ["activeTab", "tabs"],
  "action": {
    "default_popup": "popup.html"
  }
}
